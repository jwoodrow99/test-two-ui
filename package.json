{
  "name": "test-two-ui",
  "version": "0.0.6",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run prepack",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package && cpy src/lib/themes/**/*.css dist/themes/ --parents && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./themes/*.css": "./dist/themes/*.css",
    "./ui/*": {
      "types": "./dist/components/ui/*/index.d.ts",
      "svelte": "./dist/components/ui/*/index.js",
      "default": "./dist/components/ui/*/index.js"
    },
    "./custom/*": {
      "types": "./dist/components/custom/*/index.d.ts",
      "svelte": "./dist/components/custom/*/index.js",
      "default": "./dist/components/custom/*/index.js"
    },
    "./layout/*": {
      "types": "./dist/components/layout/*/index.d.ts",
      "svelte": "./dist/components/layout/*/index.js",
      "default": "./dist/components/layout/*/index.js"
    }
  },
  "peerDependencies": {
    "@internationalized/date": "^3.10.0",
    "@lucide/svelte": "^0.544.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.18",
    "@tanstack/table-core": "^8.21.3",
    "bits-ui": "^2.14.1",
    "embla-carousel-svelte": "^8.6.0",
    "formsnap": "^2.0.1",
    "layerchart": "^2.0.0-next.27",
    "mode-watcher": "^1.1.0",
    "paneforge": "^1.0.2",
    "svelte": "^5.39.5",
    "svelte-sonner": "^1.0.5",
    "sveltekit-superforms": "^2.28.0",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.4.0",
    "vaul-svelte": "^1.0.0-next.7"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^3.1.1"
  },
  "devDependencies": {
    "@storybook/addon-svelte-csf": "^5.0.10",
    "@storybook/addon-themes": "^10.0.1",
    "@storybook/sveltekit": "^10.0.1",
    "@sveltejs/adapter-auto": "^6.1.0",
    "@sveltejs/kit": "^2.43.2",
    "@sveltejs/package": "^2.5.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.0",
    "@tailwindcss/vite": "^4.1.13",
    "@types/d3": "^7.4.3",
    "@types/node": "^22",
    "cpy-cli": "^6.0.0",
    "publint": "^0.3.13",
    "storybook": "^10.0.1",
    "svelte-check": "^4.3.2",
    "typescript": "^5.9.2",
    "vite": "^7.1.7"
  },
  "keywords": [
    "svelte"
  ]
}
